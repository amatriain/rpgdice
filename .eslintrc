{
	"extends": [
		"eslint:recommended", "plugin:react/recommended"
	], 
	"parser": "babel-eslint", 
	"env": {
		"browser": true,
		"node": true
	}, 
	"plugins": [
		"react"
	], 
	"rules": {
		"no-console": 0, 
		"no-undef": 0,
		"react/display-name": 0,
		"no-underscore-dangle": 1, 
		"quotes": [2, "single"]
	}
}
